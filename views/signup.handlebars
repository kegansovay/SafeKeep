<body>
  
<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <a class="navbar-brand" href="/maker">
        <img src="/assets/img/logo.svg" width="30" height="30" class="d-inline-block align-top" alt="">
        SafeKeep
      </a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item">
            <a class="nav-link" href="#">About</a>
          </li>
        </ul>
         <form class="form-inline my-2 p-2 my-lg-0">
          <a class="btn btn-outline-success my-2 my-sm-0" href="/signup">Sign up</a>
        </form>
        <form class="form-inline my-2 p-2 my-lg-0">
          <a class="btn btn-success my-2 my-sm-0" href="/login">Login</a>
        </form>
      </div>
    </nav>

    <div class="wrapper animated zoomIn">
      <div id="formContent">
        <br>
        <br>
        <h2>SafeKeep</h2>
        <h5>Sign up</h5>
        <!-- Logo -->
        <div class="">
          <img src="/assets/img/logo.svg" style="width: 60%;" class="p-4" id="icon" alt="User Icon" />
        </div>
        
        

        <!-- Signup Form -->
        <section id="signup">
          <form id="signupForm" name="signupForm" action="/signup" method="POST">
            <input id="user" type="text"  name="username" placeholder="username">
            <input id="pass" type="password"  name="pass" placeholder="password">
            <input id="pass2" type="password"  name="pass2" placeholder="re-enter password">
            <input type="hidden" name="_csrf" value={{csrfToken}} />
            <input type="submit" class="fadeIn btn btn-success" value="Sign up">
          </form>
        </section>
        

      

      </div>
    </div>

 


